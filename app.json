{
  "expo": {
    "name": "Celo Crowdfunding",
    "slug": "celo-crowdfunding",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/white-wallet-rings.png",
    "scheme": "celodappkit",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Crowdfunding for you and your community, on Celo",
    "githubUrl": "https://github.com/Alex-Neo-Projects/celo-crowdfunding"
  }
}
